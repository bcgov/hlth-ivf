{
  "activeBrandingSetId" : "62ede3cd-273d-4be4-a8c9-387c5af80daf",
  "customCSS" : ".footer-main {\n    background-color: white;\n    padding: 20px 30px;\n    display: flex;\n    justify-content: space-between; /* Ensures spacing between elements */\n    align-items: center;\n    flex-wrap: wrap;\n}\n\n.footer-links {\n    display: flex;\n    flex-direction: column; /* Stack links in a column */\n    text-align: right; /* Aligns text to the right */\n    margin-left: auto; /* Pushes the section to the right */\n}\n\n.footer-column h4 {\n    font-size: 14px;\n    margin-bottom: 8px;\n    font-weight: bold;\n}\n\n.footer-column a {\n    display: block;\n    font-size: 14px;\n    color: #0056b3;\n    text-decoration: none;\n    margin-bottom: 5px;\n}\n\n.footer-column a:hover {\n    text-decoration: underline;\n}\n\n\n\n/* MAIN CONTAINER */\n.container {\n    font-family: Arial, sans-serif;\n    max-width: 100%;\n    margin: 0 auto;\n    background: #ffffff;\n}\n\n/* SMALLER HEADER */\n.header {\n    width: 100%;\n    background: white;\n    padding: 5px 0; /* Reduced padding */\n}\n\n.header-content {\n    display: flex;\n    justify-content: flex-start; /* Align left */\n    align-items: center;\n    max-width: 85%; /* Match the banner width */\n    margin: 0 auto;\n    padding: 0 20px;\n}\n\n/* SMALLER LOGO */\n.logo {\n    width: 120px; /* Reduce logo size */\n    height: auto;\n    margin-right: 10px;\n}\n\n/* PORTAL TITLE ALIGNED WITH BANNER */\n.portal-title {\n    font-size: 18px;\n    font-weight: bold;\n    margin: 0;\n}\n\n/* BANNER CONTAINER */\n.banner-container {\n    position: relative;\n    width: 100%;\n    display: flex;\n    justify-content: center; /* Center the banner */\n    align-items: center;\n    padding: 20px 0;\n}\n\n/* FIX BANNER SIZE */\n.banner {\n    width: 80%; /* Keep it aligned with header */\n    height: 400px; /* Adjust height */\n    object-fit: cover;\n    border-radius: 8px;\n}\n\n/* WELCOME TEXT PROPERLY POSITIONED */\n.welcome-text {\n    position: absolute;\n    bottom: 10%; /* Adjust text placement */\n    left: 10%; /* Align with banner left edge */\n    color: white;\n    font-size: 24px;\n    font-weight: bold;\n    text-align: left;\n    padding: 10px 20px;\n    border-radius: 5px;\n    max-width: 40%; /* Prevent text from exceeding banner */\n}\n\n\n.login-section {\n    text-align: center;\n    padding: 20px;\n    background: white;\n    margin-top: 20px;\n    border-radius: 5px;\n}\n\n.login-button {\n    background: #002147;\n    color: white;\n    border: none;\n    padding: 12px 18px;\n    cursor: pointer;\n    border-radius: 5px;\n    font-size: 16px;\n}",
