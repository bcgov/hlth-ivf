<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Previously_received_funding_for_IVF</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 NOT(ISNEW()),
 ISPICKVAL(PreviouslyReceivedFundingforIVF__c, &quot;Yes&quot;),
 ISBLANK(Details__c) 
)</errorConditionFormula>
    <errorMessage>This application will be marked as an exception case. Please fill out the details under the Exception Request section.</errorMessage>
</ValidationRule>
