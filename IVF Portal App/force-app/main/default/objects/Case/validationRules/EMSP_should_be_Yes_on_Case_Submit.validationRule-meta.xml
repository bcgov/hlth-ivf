<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EMSP_should_be_Yes_on_Case_Submit</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISNEW()),
NOT(ISPICKVAL(Applicant1EnrolledinMedicalServicesPlan__c, &quot;Yes&quot;))
)</errorConditionFormula>
    <errorMessage>Applicant 1 should be enrolled in MSP to be eligible for IVF funding.</errorMessage>
</ValidationRule>
