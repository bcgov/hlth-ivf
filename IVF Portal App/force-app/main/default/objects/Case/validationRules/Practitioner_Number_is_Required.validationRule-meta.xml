<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Practitioner_Number_is_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISNEW()),
ISBLANK(PractitionerNumber__c)
)</errorConditionFormula>
    <errorMessage>Practitioner Number is Required</errorMessage>
</ValidationRule>
