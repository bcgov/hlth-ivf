<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ECTP_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISNEW()),
ISBLANK(EstimatedCostofTreatmentPlan__c)
)</errorConditionFormula>
    <errorMessage>Estimated Cost of Treatment Plan is Required</errorMessage>
</ValidationRule>
